{
  "name": "yarnWithGulp",
  "dependencies": {
    "jquery": "^3.4.1",
    "gsap": "^2.1.3",
    "scrollmagic": "^2.0.7",
    "fancybox": "^3.5.7",
    "swiper": "^5.3.0"
  },
  "overrides": {
    "gsap": {
      "main": [
        "./src/uncompressed/TweenMax.js",
        "./src/uncompressed/plugins/ScrollToPlugin.js"
      ]
    },
    "scrollmagic": {
      "main": [
        "./scrollmagic/uncompressed/ScrollMagic.js",
        "./scrollmagic/uncompressed/plugins/animation.gsap.js"
      ]
    },
    "swiper": {
      "main": [
        "./package/js/swiper.min.js",
        "./package/css/swiper.min.css"
      ]
    },
    "fancybox": {
      "main": [
        "./dist/jquery.fancybox.js",
        "./dist/jquery.fancybox.css"
      ]
    }
  }
}